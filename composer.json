{
  "name": "mmerlijn/msg-repo",
  "description": "Dutch healthcare read/write edifact to/from repository",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Menno",
      "email": "menno@mmerlijn.nl"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1",
    "nesbot/carbon": "v2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.x-dev"
  },
  "autoload": {
    "psr-4": {
      "mmerlijn\\msgRepo\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "mmerlijn\\msgRepo\\tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-f": "vendor/bin/phpunit --filter"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
